<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Go Update</title>
</head>

<body>
    <table border="1" cellpadding="10">
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Designation</th>
            <th>Address</th>
            <th>Phone Number</th>
            <th>City</th>
            <th>Email</th>
            <th>Gender</th>
        </tr>
        <% users.forEach((entry)=>{%>
            <tr>
                <td>
                    <%=entry.bid%>
                </td>
                <td>
                    <%=entry.first_name%>
                </td>
                <td>
                    <%=entry.last_name%>
                </td>
                <td>
                    <%=entry.designation%>
                </td>
                <td>
                    <%=entry.address_1%>
                </td>
                <td>
                    <%=entry.phone_number%>
                </td>
                <td>
                    <%=entry.city%>
                </td>
                <td>
                    <%=entry.email%>
                </td>
                <td>
                    <%=entry.gender%>
                </td>

                <td><a href="/form/<%=entry.bid%>">Edit</a></td>
            </tr>
            <% }) %>
    </table>
</body>

</html>